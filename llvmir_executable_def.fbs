namespace iree;

// 'LLVMIR Executable'.

file_identifier "LLVM";
file_extension "ll";

// Machine independent LLVMIR executable module.
// This exeuctable will be compiled with the target machine later on.
table LLVMIRExecutableDef {
  llvmir_module:[byte];
}

root_type LLVMIRExecutableDef;
